{
  "name": "github-contributions-generator",
  "version": "1.0.0",
  "description": "Automated GitHub contribution generator with scheduling support",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "once": "node src/index.js",
    "scheduled": "cross-env SCHEDULE_ENABLED=true node src/index.js",
    "backfill": "cross-env BACKFILL_ENABLED=true node src/index.js",
    "dry-run": "cross-env DRY_RUN=true node src/index.js",
    "docker:build": "docker build -t github-contributions .",
    "docker:run": "docker run -d --env-file .env github-contributions"
  },
  "keywords": [
    "github",
    "contributions",
    "commits",
    "automation",
    "scheduler"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "jsonfile": "^6.1.0",
    "moment-timezone": "^0.5.44",
    "node-cron": "^3.0.3",
    "simple-git": "^3.22.0",
    "winston": "^3.11.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
